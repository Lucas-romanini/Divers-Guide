
@{
    ViewBag.Title = "Index";
}
<style>
    #map {
        height: 400px;
        width: 100%;
    }
</style>

<div id="style-selector-control" class="map-control">
    <select id="style-selector" class="selector-control">
        <option value="default">Default</option>
        <option value="retro" selected="selected">Retro</option>
        <option value="light">Light blue</option>
        <option value="hiding">Hide features</option>
    </select>
</div>
<div id="map"></div>
<script src="~/Content/JS/JavaScriptMap.js"></script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBw18Gwaobb2MNhMcbqOtGYodUEhdbBRZc&callback=initMap"
        async defer></script>

